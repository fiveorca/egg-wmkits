{
  "name": "egg-wmkits",
  "version": "1.0.1",
  "description": "wmkits基础框架",
  "dependencies": {
    "egg": "^3",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2",
    "egg-sequelize": "^6.0.0",
    "momment": "^0.0.1",
    "mysql2": "^3.4.0"
  },
  "devDependencies": {
    "egg-bin": "^5",
    "egg-ci": "^2",
    "egg-mock": "^5",
    "eslint": "^8",
    "eslint-config-egg": "^12"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-wmkits",
    "stop": "egg-scripts stop --title=egg-server-wmkits",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "ci": {
    "version": "16, 18",
    "type": "github"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fiveorca/egg-wmkits"
  },
  "keywords": [
    "egg",
    "egg-framework",
    "egg-wmkits"
  ],
  "author": "fiveorca",
  "files": [
    "index.js",
    "app.js",
    "lib",
    "app",
    "config"
  ],
  "eslintIgnore": [
    "coverage",
    "dist"
  ],
  "license": "MIT"
}
